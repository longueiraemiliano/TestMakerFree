{
  "Auth": {
    "Jwt": {
      "Issuer": "http://localhost:59752/",
      "Audience": "http://localhost:59752/",
      "TokenExpirationInMinutes": 1
    }
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information"
      }
    },
    "StaticFiles": {
      "Headers": {
        "Cache-Control": "no-cache, no-store",
        "Pragma": "no-cache",
        "Expires": "-1"
      }
    }
  }
}
